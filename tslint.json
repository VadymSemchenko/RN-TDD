{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "quotemark": [true, "single", "jsx-double"],
    "member-access": false,
    "no-boolean-literal-compare": false
  }
}
